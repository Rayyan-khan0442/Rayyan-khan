#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> numbers = {10, 20, 30, 40, 50};

    cout << "Traditional for loop: ";
    for (int i = 0; i < numbers.size(); i++)
        cout << numbers[i] << " ";
    cout << endl;

    cout << "Range-based for loop: ";
    for (int num : numbers)
        cout << num << " ";
    cout << endl;

    return 0;
}
