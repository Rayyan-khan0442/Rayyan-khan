#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N;
    cout << "Enter number of elements: ";
    cin >> N;

    vector<int> nums;
    cout << "Enter elements:\n";
    for (int i = 0; i < N; i++) {
        int x;
        cin >> x;
        nums.push_back(x);
    }

    for (int i = 0; i < nums.size(); ) {
        if (nums[i] % 2 == 0)
            nums.erase(nums.begin() + i);
        else
            i++;
    }

    cout << "After removing even numbers: ";
    for (int i = 0; i < nums.size(); i++)
        cout << nums[i] << " ";
    cout << endl;

    return 0;
}
