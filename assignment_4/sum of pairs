#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N, target;
    cout << "Enter number of elements: ";
    cin >> N;

    vector<int> nums(N);
    cout << "Enter elements:\n";
    for (int i = 0; i < N; i++)
        cin >> nums[i];

    cout << "Enter target sum: ";
    cin >> target;

    int count = 0;
    for (int i = 0; i < N; i++) {
        for (int j = i+1; j < N; j++) {
            if (nums[i] + nums[j] == target)
                count++;
        }
    }

    cout << "Number of pairs with sum " << target << ": " << count << endl;

    return 0;
}
